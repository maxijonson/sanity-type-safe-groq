{
  "name": "sanity-type-safe-groq",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "sanity:typegen": "sanity schema extract --path './src/sanity/sanity.schema.json' --enforce-required-fields --workspace=production && sanity typegen generate",
    "sanity:schemas": "npm run sanity:typegen && ts-to-zod src/sanity/sanity.types.ts src/sanity/sanity.schemas.ts && tsx scripts/sanity-schemas.ts"
  },
  "dependencies": {
    "@sanity/image-url": "^1.0.2",
    "@sanity/vision": "^3.52.4",
    "groqd": "^0.15.12",
    "next": "14.2.5",
    "next-sanity": "^9.4.3",
    "react": "^18",
    "react-dom": "^18",
    "sanity": "^3.52.4",
    "styled-components": "^6.1.12"
  },
  "devDependencies": {
    "@sanity/cli": "^3.52.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-to-zod": "^3.10.0",
    "tsx": "^4.16.5",
    "typescript": "^5"
  }
}
